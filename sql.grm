$OREXPRESSION = $COMPARAISON | $COMPARAISON $OROP $COMPARAISON 
$ANDEXPRESSION = $COMPARAISON | $OREXPRESSION | $NOTOP $ANDEXPRESSION
$COMPARAISON = $TERM $COMPOP $TERM 
$COMPOP = > | = | < 
$OROP =  or  
$NOTOP =  not  
$TERM = $NUMBER | $STRING | $QUOTE $TERM $QUOTE 
$NUMBER = [0-9]+ 
$STRING = [a-z]+ 
$QUOTE = " 
